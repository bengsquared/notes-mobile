#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ben/Documents/notes-mobile/node_modules/.pnpm/electron@28.3.3/node_modules/electron/node_modules:/Users/ben/Documents/notes-mobile/node_modules/.pnpm/electron@28.3.3/node_modules:/Users/ben/Documents/notes-mobile/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ben/Documents/notes-mobile/node_modules/.pnpm/electron@28.3.3/node_modules/electron/node_modules:/Users/ben/Documents/notes-mobile/node_modules/.pnpm/electron@28.3.3/node_modules:/Users/ben/Documents/notes-mobile/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../electron/cli.js" "$@"
else
  exec node  "$basedir/../electron/cli.js" "$@"
fi
